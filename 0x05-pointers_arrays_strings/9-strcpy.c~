#include "main.h"

/**
 * _strcpy - store a string in onther address
 *
 * @dest: New string
 *
 * @src: The main string
 *
 * Return: The pointer to dist
 */

char *_strcpy(char *dest, char *src)
{
	int i = 0;
	if (!*src)
	{
		return dest;
	}
	while (*(src + i) != '\0')
	{
		*(dest + i) = *(src + i);
		i++;
	}
	*(dest + i) = '\0';
	return dest;
}
